<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.yedam.prjdb.shoes.map.ShoesMapper">
	<select id="shoesSelectList" resultType="co.yedam.prjdb.shoes.sevice.ShoesVO">
		SELECT * FROM SHOES
	</select>
	<select id="shoesSelect">
		SELECT * FROM SHOES
		WHERE SHOES_ID = #{shoesId}
	</select>
</mapper>